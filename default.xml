<?xml version="1.0" encoding="UTF-8"?>
<manifest>


        <!-- ************ REMOTES ************ -->
        <!-- My aliases  -->
        <remote name="github_shapcho" fetch="https://review.gerrithub.io/shapcho" />
        
        <!-- Vim plugins  -->
        <remote name="github_scrooloose" fetch="https://github.com/scrooloose" />
        <remote name="github_vimscripts" fetch="https://github.com/vim-scripts" />
        <remote name="github_vundle" fetch="https://github.com/VundleVim" />
        <remote name="github_valloric" fetch="https://github.com/Valloric" />
        <remote name="github_tpope" fetch="https://github.com/tpope" />
        <remote name="github_rdnetto" fetch="https://github.com/rdnetto" />
        <remote name="github_will133" fetch="https://github.com/will133" />
	<remote name="github_mhinz" fetch="https://github.com/mhinz" />
                                             
        <!-- Tools  -->
        <remote name="github_mxe"     fetch="https://github.com/mxe" />


        <!-- ************ Default ************ -->
        <default 
                revision="refs/heads/master"
                remote="github_shapcho"
                sync-j="4"
        />


        <!-- ************ PROJECTS ************ -->
        <!-- My aliases  -->
        <project name="linux_aliases" remote="github_shapcho" path="kdefs/linux_aliases" />
        <project name="local_aliases" remote="github_shapcho" path="kdefs/local_aliases" />

        <!-- Vim plugins  -->
        <project name="nerdtree"       remote="github_scrooloose" path="vim/bundle/nerdtree" />
        <project name="taglist.vim"    remote="github_vimscripts" path="vim/bundle/taglist.vim" />
        <project name="Vundle.vim"     remote="github_vundle"     path="vim/bundle/Vundle.vim" />
        <project name="YouCompleteMe"  remote="github_valloric"   path="vim/bundle/YouCompleteMe" />
        <project name="vim-surround"   remote="github_tpope"      path="vim/bundle/vim-surround" />
        <project name="YCM-Generator"  remote="github_rdnetto"    path="vim/bundle/YCM-Generator" revision="stable" />
        <project name="vim-dirdiff"    remote="github_will133"    path="vim/bundle/vim-dirdiff" />
        <project name="vim-startify"   remote="github_mhinz"      path="vim/bundle/vim-startify" />

        <!-- Tools  -->
        <project name="mxe" remote="github_mxe" path="tools" />

</manifest>

